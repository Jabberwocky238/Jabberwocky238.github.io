{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
    "paths": { //是否配置paths
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/**/*"],
}